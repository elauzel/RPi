end

begin
button = KEY_UP
prog = irexec
config = echo -n t > "/home/pi/.config/pianobar/ctl"
end

begin
button = KEY_STOP
prog = irexec
config = su -c "pianobar &" -s /bin/sh pi
config = echo -n q > "/home/pi/.config/pianobar/ctl"
end

