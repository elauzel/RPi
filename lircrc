begin
button = KEY_VOLUMEDOWN
prog = irexec
config = echo -n ( > "/home/pi/.config/pianobar/ctl"
end

begin
button = KEY_PLAYPAUSE
prog = irexec
config = echo -n p > "/home/pi/.config/pianobar/ctl"
end

begin
button = KEY_VOLUMEUP
prog = irexec
config = echo -n ) > "/home/pi/.config/pianobar/ctl"
end

